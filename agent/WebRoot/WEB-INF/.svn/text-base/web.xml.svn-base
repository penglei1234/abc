<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<servlet>
		<servlet-name>keywordList</servlet-name>
		<servlet-class>com.servlet.KeywordListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>keywordList</servlet-name>
		<url-pattern>/keywordList</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>keywordAdd</servlet-name>
		<servlet-class>com.servlet.KeywordAddServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>keywordAdd</servlet-name>
		<url-pattern>/keywordAdd</url-pattern>
	</servlet-mapping>
	
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>WebSiteServlet</servlet-name>
    <servlet-class>com.servlet.WebSiteServlet</servlet-class>
  </servlet>


	<display-name></display-name>
	
	<filter>
		<filter-name>encoding</filter-name>
		<filter-class>com.filter.EncodingFilter</filter-class>
	</filter>
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.servlet.LoginServlet</servlet-class>
	</servlet>

	<filter-mapping>
		<filter-name>encoding</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
    <servlet-name>user</servlet-name>
    <servlet-class>com.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FinaceTbServlet</servlet-name>
    <servlet-class>com.servlet.FinaceTbServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ReportListServlet</servlet-name>
    <servlet-class>com.servlet.ReportListServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>user</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>role</servlet-name>
    <servlet-class>com.servlet.RoleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>role</servlet-name>
    <url-pattern>/role</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>menu</servlet-name>
    <servlet-class>com.servlet.MenuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>menu</servlet-name>
    <url-pattern>/menu</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WebSiteServlet</servlet-name>
    <url-pattern>/webSite</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FinaceTbServlet</servlet-name>
    <url-pattern>/finaceTb</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReportListServlet</servlet-name>
    <url-pattern>/reportList</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>CustomerTypeTbServlet</servlet-name>
  <servlet-class>com.servlet.CustomerTypeTbServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>CustomerTypeTbServlet</servlet-name>
  <url-pattern>/customer</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>imgCode</servlet-name>
  	<servlet-class>com.servlet.CreateImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>imgCode</servlet-name>
  	<url-pattern>/image</url-pattern>
  </servlet-mapping>


	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
